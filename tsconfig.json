{
    "compilerOptions": {
      "target": "ESNext",
      "useDefineForClassFields": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": false,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react-jsx",
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "./src/*"
        ]
      }
    },
    "include": [
      "src"
    ]
  }